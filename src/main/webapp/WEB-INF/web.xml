<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="https://jakarta.ee/xml/ns/jakartaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd"
   version="6.0">
    
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>murach.download.DownloadServlet</servlet-class>
        <!-- Adjust package if necessary -->
    </servlet>
    <servlet>
        <servlet-name>Product</servlet-name>
        <servlet-class>murach.business.Product</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductIO</servlet-name>
        <servlet-class>murach.data.ProductIO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>CookieUtil</servlet-name>
        <servlet-class>murach.until.CookieUtil</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserIO</servlet-name>
        <servlet-class>murach.data.UserIO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>murach.business.User</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/DownloadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CookieUtil</servlet-name>
        <url-pattern>/CookieUtil</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserIO</servlet-name>
        <url-pattern>/UserIO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Product</servlet-name>
        <url-pattern>/Product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductIO</servlet-name>
        <url-pattern>/ProductIO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
